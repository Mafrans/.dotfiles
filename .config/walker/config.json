{
  "placeholder": "Search...",
  "keep_open": false,
  "notify_on_fail": true,
  "enable_typeahead": false,
  "show_initial_entries": true,
  "force_keyboard_focus": true,
  "fullscreen": true,
  "activation_mode": {
    "disabled": false,
    "use_alt": false
  },
  "search": {
    "delay": 0,
    "hide_icons": true,
    "hide_spinner": true
  },
  "runner": {
    "excludes": ["rm"]
  },
  "clipboard": {
    "max_entries": 10,
    "image_height": 300
  },
  "align": {
    "width": 400,
    "horizontal": "center",
    "vertical": "center",
    "margins": {
      "top": 0,
      "bottom": 0,
      "end": 0,
      "start": 0
    }
  },
  "list": {
    "height": 300,
    "margin_top": 10,
    "always_show": true
  },
  "orientation": "vertical",
  "icons": {
    "hide": false,
    "size": 28,
    "image_height": 200
  },
  "modules": [
    {
      "name": "runner",
      "prefix": "!"
    },
    {
      "name": "applications",
      "prefix": ""
    },
    {
      "name": "ssh",
      "prefix": "",
      "switcher_exclusive": true
    },
    {
      "name": "commands",
      "prefix": "/"
    },
    {
      "name": "websearch",
      "prefix": "?"
    },
    {
      "name": "switcher",
      "prefix": "="
    }
  ]
}
